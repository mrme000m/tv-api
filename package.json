{
  "name": "@mathieuc/tradingview",
  "version": "3.5.2",
  "description": "Tradingview instant stocks API, indicator alerts, trading bot, and more !",
  "main": "main.js",
  "types": "src/types.d.ts",
  "exports": {
    ".": {
      "types": "./src/types.d.ts",
      "require": "./main.js"
    },
    "./taExtension": {
      "types": "./src/taExtension.d.ts",
      "require": "./src/taExtension.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "serve:tools": "node tools/sh/server.js",
    "serve:tools:dev": "nodemon tools/sh/server.js",
    "example": "node --env-file=.env",
    "example:dev": "nodemon --env-file=.env",
    "postinstall": "pnpm run prepare",
    "prepare": "husky install",
    "clean": "rm -rf node_modules",
    "install:pnpm": "pnpm install",
    "update:deps": "pnpm update",
    "reinstall": "pnpm run clean && pnpm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mathieu2301/TradingView-API.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "tradingwiew",
    "market",
    "stocks",
    "crypto",
    "forex",
    "indicator",
    "bitcoin",
    "api"
  ],
  "author": "Mathieu Colmon",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "jszip": "^3.10.1",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "@mathieuc/console": "^1.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "vitest": "^0.31.4"
  },
  "pnpm": {
    "overrides": {},
    "packageExtensions": {}
  },
  "volta": {
    "node": "24.11.1"
  }
}
